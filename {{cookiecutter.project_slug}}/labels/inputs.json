{
    "inputs": {
        {% for i in range(cookiecutter.number_of_inputs|int) %}
        {%- if i > 0 -%},{%- endif %}
        "input_{{ i+1 }}": {
            "displayOrder": {{ i+1 }},
            "label": "input_{{ i+1 }}_label",
            "description": "The input {{ i+1 }} description",
            "type": "Any type from [number, integer, boolean, string, data:*/*]",
            "defaultValue": "some_value(optional)",
            "fileToKeyMap": {
                "somefilename.ext":"input_{{ i+1 }}-optional"
            }
        }
        {% endfor %}
    }
}
