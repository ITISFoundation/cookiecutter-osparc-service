{
    "outputs": {
        {% for i in range(cookiecutter.number_of_outputs|int) %}
        {%- if i > 0 -%},{%- endif %}
        "output_{{ i+1 }}": {
            "displayOrder": {{ i+1 }},
            "label": "output_{{ i+1 }}_label",
            "description": "The input {{ i+1 }} description",
            "type": "string",
            "fileToKeyMap": {
                "somefilename.ext":"output_{{ i+1 }}"
            }
        }
        {% endfor %}
    }
}
